<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name = "Test Suite" parallel="false">
    <test name="SmokeTest">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <packages>
            <package name="tests"></package>
        </packages>
    </test>
</suite>